{
  "name": "@nuxtjs/sanity",
  "version": "1.2.2",
  "license": "MIT",
  "repository": "nuxt-community/sanity-module",
  "description": "Sanity integration for Nuxt",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "sanity",
    "sanity-io",
    "cms"
  ],
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./package.json": "./package.json",
    "./runtime/*": "./dist/runtime/*"
  },
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "prepare": "husky install",
    "build": "nuxt-module-build",
    "lint": "eslint --fix --ext .js,.ts,.vue .",
    "prepublishOnly": "yarn test",
    "test": "yarn lint && yarn test:unit",
    "test:unit": "vitest run test/unit/*.test.*"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.4",
    "@portabletext/types": "^1.0.3",
    "chalk": "^5.0.1",
    "defu": "^6.0.0",
    "fs-extra": "^10.1.0",
    "knitwork": "^0.1.2",
    "ohash": "^0.1.0",
    "pathe": "^0.3.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.1.7",
    "@nuxt/schema": "^3.0.0-rc.4",
    "@nuxt/test-utils": "npm:@nuxt/test-utils-edge@latest",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@release-it/conventional-changelog": "^5.0.0",
    "@sanity/client": "^3.3.2",
    "@types/fs-extra": "^9.0.13",
    "@vue/test-utils": "^2.0.0",
    "c8": "^7.11.3",
    "eslint": "^8.17.0",
    "happy-dom": "^5.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "nuxi": "^3.0.0-rc.3",
    "nuxt": "npm:nuxt3@latest",
    "release-it": "^15.0.0",
    "vitest": "^0.14.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "build": {
    "externals": [
      "@sanity/client"
    ]
  },
  "packageManager": "yarn@3.2.1",
  "volta": {
    "node": "16.15.1"
  }
}
