{
  "name": "@nuxtjs/sanity",
  "version": "1.5.0",
  "license": "MIT",
  "repository": "nuxt-modules/sanity",
  "description": "Sanity integration for Nuxt",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "sanity",
    "sanity-io",
    "cms"
  ],
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://roe.dev"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./package.json": "./package.json",
    "./runtime/*": "./dist/runtime/*"
  },
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "docs:generate": "nuxi generate docs",
    "prepare": "husky install",
    "build": "nuxt-module-build",
    "lint": "eslint --fix --ext .js,.ts,.vue .",
    "prepublishOnly": "pnpm test",
    "test": "pnpm lint && pnpm vitest run",
    "test:unit": "vitest run test/unit/*.test.*",
    "test:e2e": "vitest run test/e2e/*.test.*"
  },
  "dependencies": {
    "@nuxt/kit": "3.0.0",
    "@portabletext/types": "^2.0.0",
    "chalk": "^5.1.2",
    "defu": "^6.1.1",
    "fs-extra": "^10.1.0",
    "knitwork": "^1.0.0",
    "ohash": "^1.0.0",
    "pathe": "^1.0.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "0.2.1",
    "@nuxt/schema": "3.0.0",
    "@nuxt/test-utils": "3.0.0",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "@sanity/client": "^3.4.1",
    "@types/fs-extra": "^9.0.13",
    "@vitest/coverage-c8": "^0.25.2",
    "@vue/test-utils": "^2.2.2",
    "c8": "^7.12.0",
    "eslint": "^8.27.0",
    "happy-dom": "^7.7.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "nuxi": "3.0.0",
    "nuxt": "3.0.0",
    "release-it": "^15.5.0",
    "typescript": "^4.9.3",
    "vitest": "^0.25.2",
    "vue": "^3.2.45"
  },
  "resolutions": {
    "@nuxtjs/sanity": "link:.",
    "@nuxtjs/tailwindcss": "^6.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "build": {
    "externals": [
      "@sanity/client"
    ]
  },
  "packageManager": "pnpm@7.16.0",
  "volta": {
    "node": "18.12.1"
  }
}
