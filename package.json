{
  "name": "@nuxtjs/sanity",
  "version": "1.1.1",
  "license": "MIT",
  "repository": "nuxt-community/sanity-module",
  "description": "Sanity integration for Nuxt",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "sanity",
    "sanity-io",
    "cms"
  ],
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./package.json": "./package.json",
    "./runtime/*": "./dist/runtime/*"
  },
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "prepare": "husky install",
    "build": "nuxt-module-build",
    "lint": "eslint --fix --ext .js,.ts,.vue .",
    "prepublishOnly": "yarn test",
    "test": "yarn lint && yarn test:unit",
    "test:unit": "vitest test/unit/*.test.*"
  },
  "dependencies": {
    "@nuxt/kit": "npm:@nuxt/kit-edge@^3.0.0-27420403.16e2a54",
    "@portabletext/types": "1.0.3",
    "chalk": "5.0.0",
    "defu": "5.0.1",
    "fs-extra": "10.0.1",
    "knitwork": "0.1.0",
    "murmurhash-es": "0.1.1",
    "pathe": "0.2.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "0.1.7",
    "@nuxtjs/eslint-config-typescript": "8.0.0",
    "@release-it/conventional-changelog": "4.2.0",
    "@sanity/client": "3.1.0",
    "@types/fs-extra": "9.0.13",
    "@vue/test-utils": "2.0.0-rc.18",
    "c8": "7.11.0",
    "eslint": "8.10.0",
    "happy-dom": "2.41.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.5",
    "nuxi": "0.10.1",
    "nuxt3": "^3.0.0-27420403.16e2a54",
    "release-it": "14.12.5",
    "vitest": "0.5.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "build": {
    "externals": [
      "@sanity/client"
    ]
  },
  "packageManager": "yarn@3.2.0",
  "volta": {
    "node": "16.14.0"
  }
}
