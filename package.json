{
  "name": "@nuxtjs/sanity",
  "version": "0.9.2",
  "description": "Sanity integration for Nuxt.js",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "sanity",
    "sanity-io",
    "cms"
  ],
  "repository": "nuxt-community/sanity-module",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.es.js"
    },
    "./module": {
      "require": "./dist/module.js",
      "import": "./dist/module.es.js"
    },
    "./": "./",
    "./sanity-content": "./dist/components/sanity-content.js",
    "./sanity-file": "./dist/components/sanity-file.js",
    "./sanity-image": "./dist/components/sanity-image.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "module.js",
    "dist",
    "vetur",
    "templates"
  ],
  "scripts": {
    "build": "siroc build",
    "dev": "nuxt example",
    "lint": "eslint --fix --ext .js,.ts,.vue .",
    "prepare": "husky install && yarn build",
    "prepublishOnly": "yarn test",
    "test": "yarn lint && jest"
  },
  "dependencies": {
    "chalk": "4.1.1",
    "consola": "2.15.3",
    "defu": "5.0.0",
    "fs-extra": "9.1.0",
    "upath": "2.0.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.14.2",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-typescript": "7.13.0",
    "@nuxt/test-utils": "0.2.2",
    "@nuxt/types": "2.15.6",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config-typescript": "6.0.0",
    "@nuxtjs/tailwindcss": "4.1.2",
    "@release-it/conventional-changelog": "2.0.1",
    "@sanity/client": "2.8.0",
    "@types/fs-extra": "9.0.11",
    "@types/jest": "26.0.23",
    "@types/webpack-env": "1.16.0",
    "@vue/test-utils": "1.2.0",
    "babel-jest": "26.6.3",
    "eslint": "7.26.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "jest-serializer-vue-tjw": "3.18.0",
    "lint-staged": "11.0.0",
    "nuxt": "2.15.6",
    "playwright": "1.11.0",
    "release-it": "14.6.2",
    "siroc": "0.10.0",
    "ts-loader": "8.2.0"
  },
  "peerDependencies": {
    "chalk": "^3.0.0",
    "consola": "^2.15.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  },
  "volta": {
    "node": "12.22.1"
  }
}
