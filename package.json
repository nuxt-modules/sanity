{
  "name": "@nuxtjs/sanity",
  "version": "1.10.0",
  "license": "MIT",
  "repository": "nuxt-modules/sanity",
  "description": "Sanity integration for Nuxt",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "sanity",
    "sanity-io",
    "cms"
  ],
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://roe.dev"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./package.json": "./package.json",
    "./runtime/*": "./dist/runtime/*"
  },
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "studio:build": "pnpm --filter nuxt-sanity-cms build",
    "docs:generate": "nuxi generate docs",
    "prepare": "simple-git-hooks",
    "build": "nuxt-module-build build",
    "lint": "eslint --fix --ext .js,.ts,.vue .",
    "prepublishOnly": "pnpm test",
    "release": "bumpp && npm publish",
    "test": "pnpm lint && pnpm vitest run",
    "test:types": "vue-tsc --noEmit",
    "test:unit": "vitest run test/unit/*.test.*",
    "test:e2e": "vitest run test/e2e/*.test.*"
  },
  "dependencies": {
    "@nuxt/kit": "^3.10.3",
    "@portabletext/types": "^2.0.8",
    "@sanity/client": "^6.15.2",
    "@sanity/core-loader": "^1.6.1",
    "@sanity/preview-url-secret": "^1.6.4",
    "@sanity/visual-editing": "^1.7.0",
    "chalk": "^5.3.0",
    "defu": "^6.1.4",
    "knitwork": "^1.0.0",
    "mlly": "^1.6.1",
    "ofetch": "^1.3.3",
    "ohash": "^1.1.3",
    "pathe": "^1.1.2"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "latest",
    "@nuxt/module-builder": "0.5.5",
    "@nuxt/schema": "3.10.3",
    "@nuxt/test-utils": "3.11.0",
    "@vitest/coverage-v8": "1.3.1",
    "@vue/runtime-core": "^3.4.21",
    "@vue/test-utils": "2.4.4",
    "bumpp": "9.4.0",
    "eslint": "8.57.0",
    "h3": "^1.11.1",
    "happy-dom": "13.6.2",
    "lint-staged": "15.2.2",
    "nuxi": "3.10.1",
    "nuxt": "3.10.3",
    "simple-git-hooks": "2.10.0",
    "typescript": "5.4.2",
    "vitest": "1.3.1",
    "vitest-environment-nuxt": "1.0.0",
    "vue": "3.4.21",
    "vue-router": "^4.3.0",
    "vue-tsc": "2.0.6"
  },
  "resolutions": {
    "@nuxt/schema": "3.10.3",
    "@nuxtjs/sanity": "link:."
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "publishConfig": {
    "access": "public"
  },
  "build": {
    "externals": [
      "@sanity/client"
    ]
  },
  "packageManager": "pnpm@8.15.4"
}
